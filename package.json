{
  "name": "growpos-desktop",
  "productName": "Growpos",
  "version": "0.0.1",
  "author": "spiderwave",
  "description": "pos billing app",
  "main": "index.js",
  "homepage": "./",
  "build": {
    "appId": "cool-app",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/icon.ico",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "installerIcon": "assets/icon.ico",
      "uninstallerIcon": "assets/icon.ico",
      "uninstallDisplayName": "Growpos Desktop",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "electron-builder": "^22.9.1",
    "electron-is-dev": "^1.2.0",
    "electron-pos-printer": "^1.2.4",
    "electron-serve": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "generate-serial-number": "0.0.3",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.9",
    "multer": "^1.4.2",
    "prop-types": "^15.7.2",
    "serve": "^11.3.2",
    "shelljs": "^0.7.7",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "uaup-js": "0.0.21",
    "uuid": "^8.3.0",
    "validator": "^13.1.17"
  },
  "scripts": {
    "start": " electron-forge start",
    "build": "electron-packager . Growpos --appVersion=1.0.0 --buildVersion=2.0.0 --platform win32 --arch x64 --overwrite --icon ./assets/favicon.ico  --prune=true --asar --out dist/",
    "setup": "electron-installer-windows --src dist/app-win32-x64/ --dest dist/installers/",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build-installer": "electron-builder"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "concurrently": "^5.2.0",
    "electron": "^9.0.5",
    "electron-devtools-installer": "^3.1.0",
    "electron-installer-windows": "^3.0.0",
    "electron-react-devtools": "^0.5.3",
    "path": "^0.12.7",
    "wait-on": "^5.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./assets/favicon.ico"
      },
      "electronPackagerConfig": {
        "icon": "./assets/favicon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "growpos_desktop"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
